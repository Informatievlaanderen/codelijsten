@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

<https://data.vlaanderen.be/id/conceptscheme/binairestatus>
    a skos:ConceptScheme ;
    skos:prefLabel "conceptscheme binairestatus";
    skos:definition "Codelijst voor objecten die slechts twee statussen (actueel/historisch) kennen." .

<https://data.vlaanderen.be/id/concept/binairestatus/gehistoreerd>
    a skos:Concept ;
    skos:definition "Het object is niet meer actueel." ;
    skos:inscheme <https://data.vlaanderen.be/id/conceptscheme/binairestatus> ;
    
    skos:prefLabel "gehistoreerd" ;
    skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/binairestatus> .

<https://data.vlaanderen.be/id/concept/binairestatus/gerealiseerd>
    a skos:Concept ;
    skos:definition "Het object is actueel." ;
    skos:inscheme <https://data.vlaanderen.be/id/conceptscheme/binairestatus> ;
    
    skos:prefLabel "gerealiseerd" ;
    skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/binairestatus> .

