@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

<https://data.vlaanderen.be/id/conceptscheme/gebouwstatus>
    a skos:ConceptScheme ;
    skos:prefLabel "conceptscheme gebouwstatus";
    skos:definition "Codelijst voor de status van een gebouw." .

<https://data.vlaanderen.be/id/concept/gebouwstatus/gehistoreerd>
    a skos:Concept ;
    skos:definition "Het gebouw werd gesloopt, samengevoegd of gesplitst." ;
    skos:inscheme <https://data.vlaanderen.be/id/conceptscheme/gebouwstatus> ;
    
    skos:prefLabel "gehistoreerd" ;
    skos:relatedMatch <https://inspire.ec.europa.eu/codelist/ConditionOfConstructionValue/demolished> ;
    skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/gebouwstatus> .

<https://data.vlaanderen.be/id/concept/gebouwstatus/gepland>
    a skos:Concept ;
    skos:definition "Er werd een vergunning aangevraagd of verleend voor het gebouw." ;
    skos:inscheme <https://data.vlaanderen.be/id/conceptscheme/gebouwstatus> ;
    
    skos:prefLabel "gepland" ;
    skos:relatedMatch <https://inspire.ec.europa.eu/codelist/ConditionOfConstructionValue/projected> ;
    skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/gebouwstatus> .

<https://data.vlaanderen.be/id/concept/gebouwstatus/gerealiseerd>
    a skos:Concept ;
    skos:definition "Het gebouw werd gerealiseerd (werken beëindigd) en/of is waarneembaar op het terrein." ;
    skos:inscheme <https://data.vlaanderen.be/id/conceptscheme/gebouwstatus> ;
    
    skos:prefLabel "gerealiseerd" ;
    skos:relatedMatch <https://inspire.ec.europa.eu/codelist/ConditionOfConstructionValue/functional> ;
    skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/gebouwstatus> .

<https://data.vlaanderen.be/id/concept/gebouwstatus/inAanbouw>
    a skos:Concept ;
    skos:definition "De werken aan het gebouw zijn gestart." ;
    skos:inscheme <https://data.vlaanderen.be/id/conceptscheme/gebouwstatus> ;
    
    skos:prefLabel "in aanbouw" ;
    skos:relatedMatch <https://inspire.ec.europa.eu/codelist/ConditionOfConstructionValue/underConstruction> ;
    skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/gebouwstatus> .

<https://data.vlaanderen.be/id/concept/gebouwstatus/nietGerealiseerd>
    a skos:Concept ;
    skos:definition "De vergunning voor het gebouw werd niet verleend of is vervallen of de vergunningsaanvraag werd ingetrokken door de bouwheer." ;
    skos:inscheme <https://data.vlaanderen.be/id/conceptscheme/gebouwstatus> ;
    
    skos:prefLabel "niet gerealiseerd" ;
    skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/gebouwstatus> .

