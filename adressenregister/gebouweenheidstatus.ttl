@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

<https://data.vlaanderen.be/id/conceptscheme/gebouweenheidstatus>
    a skos:ConceptScheme ;
    skos:prefLabel "conceptscheme gebouweenheidstatus";
    skos:definition "Codelijst voor de status van een gebouweenheid." .

<https://data.vlaanderen.be/id/concept/gebouweenheidstatus/gehistoreerd>
    a skos:Concept ;
    skos:definition "De gebouweenheid hield (al dan niet samen met het gebouw) op te bestaan, werd samengevoegd of gesplitst." ;
    skos:inscheme <https://data.vlaanderen.be/id/conceptscheme/gebouweenheidstatus> ;
    
    skos:prefLabel "gehistoreerd" ;
    skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/gebouweenheidstatus> .

<https://data.vlaanderen.be/id/concept/gebouweenheidstatus/gepland>
    a skos:Concept ;
    skos:definition "Er werd een vergunning aangevraagd of verleend voor de gebouweenheid." ;
    skos:inscheme <https://data.vlaanderen.be/id/conceptscheme/gebouweenheidstatus> ;
    
    skos:prefLabel "gepland" ;
    skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/gebouweenheidstatus> .

<https://data.vlaanderen.be/id/concept/gebouweenheidstatus/gerealiseerd>
    a skos:Concept ;
    skos:definition "De gebouweenheid werd gerealiseerd (werken beëindigd) en/of is waarneembaar op het terrein." ;
    skos:inscheme <https://data.vlaanderen.be/id/conceptscheme/gebouweenheidstatus> ;
    
    skos:prefLabel "gerealiseerd" ;
    skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/gebouweenheidstatus> .

<https://data.vlaanderen.be/id/concept/gebouweenheidstatus/nietGerealiseerd>
    a skos:Concept ;
    skos:definition "De vergunning voor de gebouweenheid werd niet verleend of is vervallen of de vergunningsaanvraag werd ingetrokken door de bouwheer." ;
    skos:inscheme <https://data.vlaanderen.be/id/conceptscheme/gebouweenheidstatus> ;
    
    skos:prefLabel "niet gerealiseerd" ;
    skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/gebouweenheidstatus> .
