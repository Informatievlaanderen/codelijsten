@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .


<https://data.vlaanderen.be/id/conceptscheme/verticalereferentie>
    a skos:ConceptScheme ;
    skos:prefLabel "conceptscheme verticalereferentie";
    skos:definition "Codelijst voor de verticale referentie van een geometrie." .

<https://data.vlaanderen.be/id/concept/verticalereferentie/bodem>
    a skos:Concept ;
    skos:definition "De hoogte is gemeten op bodemniveau, om het even waar tussen de laagste en de hoogste bodempunten van het gebouw." ;
    skos:inscheme <https://data.vlaanderen.be/id/conceptscheme/verticalereferentie> ;
    
    skos:prefLabel "bodem" ;
    skos:relatedMatch <https://inspire.ec.europa.eu/codelist/ElevationReferenceValue/generalGround> ;
    skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/verticalereferentie> .

<https://data.vlaanderen.be/id/concept/verticalereferentie/dak>
    a skos:Concept ;
    skos:definition "De hoogte is gemeten op dakniveau, om het even waar tussen het laagste dakrandniveau en de top van het gebouw." ;
    skos:inscheme <https://data.vlaanderen.be/id/conceptscheme/verticalereferentie> ;
    
    skos:prefLabel "dak" ;
    skos:relatedMatch <https://inspire.ec.europa.eu/codelist/ElevationReferenceValue/generalRoof> ;
    skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/verticalereferentie> .

<https://data.vlaanderen.be/id/concept/verticalereferentie/dakrand>
    a skos:Concept ;
    skos:definition "De hoogte is gemeten op dakrandniveau, ergens tussen de laagste en de hoogste dakrandniveaus van het gebouw." ;
    skos:inscheme <https://data.vlaanderen.be/id/conceptscheme/verticalereferentie> ;
    
    skos:prefLabel "dakrand" ;
    skos:relatedMatch <https://inspire.ec.europa.eu/codelist/ElevationReferenceValue/generalRoofEdge> ;
    skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/verticalereferentie> .

<https://data.vlaanderen.be/id/concept/verticalereferentie/hoogstePuntVanGebouw>
    a skos:Concept ;
    skos:definition "De hoogte is gemeten op het hoogste punt van het gebouw, inclusief de installaties, zoals schoorstenen en antennes." ;
    skos:inscheme <https://data.vlaanderen.be/id/conceptscheme/verticalereferentie> ;
    
    skos:prefLabel "hoogste punt van gebouw" ;
    skos:relatedMatch <https://inspire.ec.europa.eu/codelist/ElevationReferenceValue/highestPoint> ;
    skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/verticalereferentie> .

<https://data.vlaanderen.be/id/concept/verticalereferentie/topVanGebouw>
    a skos:Concept ;
    skos:definition "De hoogte is gemeten op de top van het gebouw." ;
    skos:inscheme <https://data.vlaanderen.be/id/conceptscheme/verticalereferentie> ;
    
    skos:prefLabel "top van gebouw" ;
    skos:relatedMatch <https://inspire.ec.europa.eu/codelist/ElevationReferenceValue/topOfConstruction> ;
    skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/verticalereferentie> .

