@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .


<https://data.vlaanderen.be/id/conceptscheme/horizontalereferentie>
    a skos:ConceptScheme ;
    skos:prefLabel "conceptscheme horizontalereferentie";
    skos:definition "Codelijst voor de horizontale referentie van een geometrie." .

<https://data.vlaanderen.be/id/concept/horizontalereferentie/bovengrondseOmhulling>
    a skos:Concept ;
    skos:definition "De coördinaten van het gebouw stellen de bovengrondse omhulling van het gebouw voor, d.w.z. de maximale omvang van het gebouw boven de grond." ;
    skos:inscheme <https://data.vlaanderen.be/id/conceptscheme/horizontalereferentie> ;
    
    skos:prefLabel "bovengrondse omhulling" ;
    skos:relatedMatch <https://inspire.ec.europa.eu/codelist/HorizontalGeometryReferenceValue/aboveGroundEnvelope> ;
    skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/horizontalereferentie> .

<https://data.vlaanderen.be/id/concept/horizontalereferentie/dakrand>
    a skos:Concept ;
    skos:definition "De coördinaten van het gebouw stellen de dakranden van het gebouw voor." ;
    skos:inscheme <https://data.vlaanderen.be/id/conceptscheme/horizontalereferentie> ;
    
    skos:prefLabel "dakrand" ;
    skos:relatedMatch <https://inspire.ec.europa.eu/codelist/HorizontalGeometryReferenceValue/roofEdge> ;
    skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/horizontalereferentie> .

<https://data.vlaanderen.be/id/concept/horizontalereferentie/gecombineerd>
    a skos:Concept ;
    skos:definition "De coördinaten van het gebouw zijn verkregen uit de combinatie van de geometrieën van zijn gebouwonderdelen, die verschillende horizontale referenties kunnen hebben." ;
    skos:inscheme <https://data.vlaanderen.be/id/conceptscheme/horizontalereferentie> ;
    
    skos:prefLabel "gecombineerd" ;
    skos:relatedMatch <https://inspire.ec.europa.eu/codelist/HorizontalGeometryReferenceValue/combined> ;
    skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/horizontalereferentie> .

<https://data.vlaanderen.be/id/concept/horizontalereferentie/voetafdruk>
    a skos:Concept ;
    skos:definition "De coördinaten van het gebouw stellen de voetafdruk van het gebouw voor, d.w.z. zijn omvang op maaiveldhoogte." ;
    skos:inscheme <https://data.vlaanderen.be/id/conceptscheme/horizontalereferentie> ;
    
    skos:prefLabel "voetafdruk" ;
    skos:relatedMatch <https://inspire.ec.europa.eu/codelist/HorizontalGeometryReferenceValue/footPrint> ;
    skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/horizontalereferentie> .

